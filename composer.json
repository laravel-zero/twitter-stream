{
    "name": "laravel-zero/twitter-stream",
    "description": "An Laravel Zero application that hears a twitter feed.",
    "keywords": ["framework", "laravel", "console", "zero"],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/laravel-zero/twitter-stream/issues",
        "source": "https://github.com/laravel-zero/twitter-stream"
    },
    "authors": [
        {
            "name": "Nuno Maduro",
            "email": "enunomaduro@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.1",
        "laravel-zero/framework": "~3.6",
        "spatie/laravel-twitter-streaming-api": "^1.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "phpunit/phpunit": "~6.3",
        "mockery/mockery": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "scripts": {
        "post-create-project-cmd": [
            "php application app:rename"
        ]
    },
    "bin": ["twitter-stream"]
}
